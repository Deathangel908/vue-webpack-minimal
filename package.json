{
  "name": "vue-wepback-typescript",
  "scripts": {
    "build:prod": "webpack --progress --mode production ",
    "build:test": "webpack --mode production --coverage true",
    "start": "webpack-dev-server --host 0.0.0.0 --mode development --hot --port 9084 --coverage true",
    "start:lint": "webpack-dev-server --host 0.0.0.0 --mode development --hot --port 9084 --coverage true --lint true",
    "test:cypress:run": "CYPRESS_baseUrl=http://localhost:9084 CYPRESS_coveragereport=true start-server-and-test 'http-server -p 9084 ./dist' 9084 'cypress run'",
    "test:cypress:debug": "CYPRESS_baseUrl=http://localhost:9084 CYPRESS_coveragereport=true cypress open",
    "test:unit": "rm -rf ./nyc/coverage-unit &&  rm -rf ./nyc/coverage-unit-output && yarn nyc --temp-dir ./nyc/coverage-unit-output --report-dir ./nyc/coverage-unit mocha",
    "test:codecov": "nyc report --reporter=text-lcov | codecov --pipe",
    "test:coverage": "nyc report --reporter=text && nyc check-coverage",
    "test": "run-s clean build:test test:cypress:run",
    "lint:style": "stylelint 'src/**/*.vue' 'src/**/*.sass'",
    "lint:es": "eslint --ext .ts,.vue --max-warnings=0 src test",
    "lint": "run-s -c lint:style lint:es",
    "fix": "eslint --fix --ext .ts,.vue src",
    "lint:new": "eslint-find-rules -u ./.eslintrc.json",
    "clean": "rm -rf ./dist && rm -rf ./nyc && rm -rf ./node_modules/.cache"
  },
  "license": "MIT",
