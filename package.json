{
  "name": "vue-wepback-typescript",
  "scripts": {
    "build:prod": "webpack --progress --mode production ",
    "build:test": "webpack --mode production --coverage true",
    "start": "webpack-dev-server --host 0.0.0.0 --mode development --hot --port 9084 --coverage true",
    "test:cypress:run": "CYPRESS_baseUrl=http://localhost:9084 CYPRESS_coveragereport=true start-server-and-test 'http-server -p 9084 ./dist' 9084 'cypress run'",
    "test:cypress:debug": "CYPRESS_baseUrl=http://localhost:9084 CYPRESS_coveragereport=true cypress open",
    "test:unit": "rm -rf ./nyc/.nyc_output_unit && rm -rf ./nyc/coverage_unit && nyc --temp-dir ./nyc/.nyc_output_unit --report-dir ./nyc/coverage_unit mocha",
    "test:cypress:build:run": "yarn run build:test && yarn run test:cypress:run",
    "test": "yarn run clean; yarn run test:unit; yarn run test:cypress:build:run; yarn run test:combine",
    "lint:css": "stylelint 'src/**/*.vue' 'src/**/*.sass'",
    "lint:ts": "eslint --ext .ts,.vue --max-warnings=0 src test cypress/integration",
    "lint": "concurrently 'yarn run lint:css' 'yarn run lint:ts'",
    "fix": "eslint --fix --ext .ts,.vue src",
    "clean": "rm -rf ./dist && rm -rf ./nyc && rm -rf ./node_modules/.cache",
    "codecov": "nyc report --temp-dir ./nyc/.nyc_output --reporter=text-lcov > coverage.lcov && codecov",
    "test:combine": "rm -rf ./nyc/.nyc_output && yarn nyc merge ./nyc/.nyc_output_cypress/ ./nyc/.nyc_output/cypress.json && yarn nyc merge ./nyc/.nyc_output_unit/ ./nyc/.nyc_output/unit.json && yarn nyc  --temp-dir ./nyc/.nyc_output --report-dir ./nyc/coverage report"
  },
  "license": "MIT",
